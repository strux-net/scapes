install_tru:
	@echo  installing tru
	@${m_copy} tru_tmp ok/tru_tmp
	@${m_copy} tru ok/tru
	@${m_copy} statistics.out ok/statistics.out
	@${m_copy} docs/README.txt ok/docs/README.txt
	@${m_copy} docs/README.html ok/docs/README.html
	@${m_copy} tru ~/strux/bin/tru
test_tru: tru
	@echo  testing tru
	./tru -version
.PHONY: tru
tru:
	@echo  building tru
	perl -I ~/strux/lib  ~/strux/bin/tru tru.tru ../lib/basicUSAGE.tru >tru_tmp
	@${m_diff} tru_tmp
	perl tru_tmp -s statistics.out tru.tru >tru
	@${m_diff} tru
	@${m_diff} statistics.out
	chmod +x tru
	{ echo "tru\n  A parser-generator for structured text\n    usage"; ./tru -? 2>&1 | i -it 1 ; } >docs/README.txt
	@${m_diff} docs/README.txt
	{ sed "s/^    usage$$/    code/" docs/README.txt ; echo "style\n  Title\tA parser-generator for structured text" ; } | html_tru >docs/README.html
	@${m_diff} docs/README.html
